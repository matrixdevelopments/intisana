{
	"name": "shinigamicorei7/intisana",
	"description": "Sistema de notas on-line",
	"keywords": [
		"framework",
		"laravel",
		"intisana",
		"online"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.5.9",
		"laravel/framework": "5.1.*"
	},
	"require-dev": {
		"barryvdh/laravel-ide-helper": "dev-master",
		"doctrine/dbal": "~2.3"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
